{
    "name": "perfectworkout/plaid-sdk-php",
    "description": "A fork of TomorrowIdeas/Plaid PHP SDK for Plaid's API.",
    "keywords": [
        "plaid",
        "sdk",
        "php"
    ],
    "type": "library",
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "ext-curl": "*",
        "nimbly/shuttle": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "TomorrowIdeas\\Plaid\\" : "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.0",
        "symfony/var-dumper": "^4.2",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "TomorrowIdeas\\Plaid\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Scheffler",
            "email": "brent@brentscheffler.com"
        },
		{
			"name": "Jordan Novak",
			"email": "jordan.novak@theperfectworkout.com"
		}
    ]
}
